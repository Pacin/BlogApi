{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "maxLength": 100,
      "minLength": 3
    },
    "body": {
      "type": "richtext",
      "required": true
    },
    "user": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "posts"
    }
  }
}
